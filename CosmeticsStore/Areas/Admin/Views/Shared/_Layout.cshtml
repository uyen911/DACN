                    </a>
                </li>

            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar control-sidebar-light elevation-3">
            <!-- Brand Logo -->

            <a href="/admin/home" class="brand-link">
                <img src="~/Content/admin/images/Thiết kế chưa có tên (1).png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-light"><strong>HPUStore</strong></span>
            </a>

            <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                <div class="image">
                    <img src="~/Content/admin/images/logo.png" class="img-circle elevation-2" alt="User Image">
                </div>
                <div class="info">
                    <a href="/admin/profile/profile/@User.Identity.Name" class="d-block">@User.Identity.Name</a>
                </div>
            </div>

            <!-- Sidebar -->
            <!-- Sidebar user panel (optional) -->

            @Html.Partial("_PartiaSideBar")

        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @RenderBody()
        </div>

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-open">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
    </div>
    <!-- ./wrapper -->
