    <!-- dropzonejs -->
    <script src="~/Content/admin/plugins/dropzone/min/dropzone.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/admin/dist/js/adminlte.min.js"></script>
