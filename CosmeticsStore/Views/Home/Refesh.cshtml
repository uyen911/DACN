
@model CosmeticsStore.Models.ThongKeModel
@if (Model != null)
{
    <div class="thongke">
        <table class="table">
            <tr>
                <td>Hôm Nay</td>
                <td>@Model.HomNay</td>
            </tr>
            <tr>
                <td>Hôm Qua</td>
                <td>@Model.HomQua</td>
            </tr>
            <tr>
                <td>Tuần Này</td>
                <td>@Model.TuanNay</td>
            </tr>
            <tr>
                <td>Tuần Trước</td>
                <td>@Model.TuanTruoc</td>
            </tr>
            <tr>
                <td>Tháng Này</td>
                <td>@Model.ThangNay</td>
            </tr>
            <tr>
                <td>Tháng Trước</td>
                <td>@Model.ThangTruoc</td>
            </tr>
            <tr>
                <td>Số Người Online</td>
                <td>@ViewBag.Visitors_online</td>
            </tr>

        </table>

    </div>
}